<h3 class="mb-4">
  Channel
</h3>

<table class="table">
  <tbody>
    <tr>
      <th scope="row">Name</th>
      <td><%= @channel.name %></td>
    </tr>
    <tr>
      <th scope="row">Description</th>
      <td><%= @channel.description %></td>
    </tr>
    <tr>
      <th scope="row">Active</th>
      <td><%= @channel.active ? 'Yes' : 'No' %></td>
    </tr>
  </tbody>
</table>

<%= link_to 'Back', admin_channels_path, class: 'btn btn-primary' %>
