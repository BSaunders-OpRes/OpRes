$('#sla-tab-content').html('<%= escape_javascript(render "organisation/business_service_lines/partials/#{params[:partial_name]}", sla_attr: params[:args][:sla_name], supplier: @service_level_agreement) %>');
if ($('.lazy-graph').length >= 1) {
  $('.lazy-graph').each(function() {
    $.ajax({
      url:      '/organisation/graphs/compose',
      dataType: 'script',
      type:     'GET',
      async:     false,
      data:     {
        key:            $(this).data('key'),
        args:           $(this).data('args'),
        data_append_to: $(this).data('append-to'),
        partial_name:   $(this).data('partial-name')
      }
    });
  });
}
