<%= form_for BusinessServiceLine.new, url: import_organisation_business_service_lines_path, multipart: true do |f| %>
   <div class="form-group">
      <%= f.label :file, "Upload CSV File" %>
      <%= f.file_field :file, required: true, accept: 'text/csv' %>
   </div>
   <%= f.button :submit %>
<% end %>
