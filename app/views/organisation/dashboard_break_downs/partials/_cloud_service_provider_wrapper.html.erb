                </div>
              </div>
              <div class="col-lg-6 col-md-12">
                <div class="box">
                  <div id="consumption-chp" class="order-1 mb-lg-0 mb-4 px-1 custom-high-chart lazy-graph" data-key="consumption_chp" data-args="<%= {}.to_json %>" data-append-to="consumption-chp" data-partial-name="consumption_chp" data-partial-name="consumption_chp" data-filters="<%= filters.to_json %>">
                      <%= render 'organisation/graphs/partials/consumption_chp', data: {} %>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
