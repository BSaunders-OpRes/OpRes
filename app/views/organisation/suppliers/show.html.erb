                        <tr>
                          <td width='300px' class="font-13 px-4">
                            Incident Response SLA
                          </td>
                          <td class="font-13 px-4">
                            <%= @supplier.sla&.severity1 %> Mins
                          </td>
                          <td class="font-13 px-4">
                            <%= @supplier.sla&.severity2 %> Mins
                          </td>
                          <td class="font-13 px-4">
                            <%= @supplier.sla&.severity3 %> Mins
                          </td>
                          <td class="font-13 px-4">
                            <%= @supplier.sla&.severity4 %> Mins
                          </td>
                        </tr>
                        <tr>
                          <td width='300px' class="font-13 px-4">
                            Incident Restoration SLA
                          </td>
                          <td class="font-13 px-4">
                            <%= @supplier.sla&.severity1_restoration %> Mins
                          </td>
                          <td class="font-13 px-4">
                            <%= @supplier.sla&.severity2_restoration %> Mins
                          </td>
                          <td class="font-13 px-4">
                            <%= @supplier.sla&.severity3_restoration %> Mins
                          </td>
                          <td class="font-13 px-4">
                            <%= @supplier.sla&.severity4_restoration %> Mins
                          </td>
                        </tr>
                      </tbody>
                    </table>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div id="section3">
