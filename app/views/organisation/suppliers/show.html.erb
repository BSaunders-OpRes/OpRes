<div id='supplier-view' class="new-dashboard new-cloud-dashboard supplier-show">
  <div class="row no-gutters">
    <div class="col-sm-12 px-1">
      <div class="d-flex justify-content-between align-items-center flex-wrap">
        <div class="left d-flex align-items-center flex-wrap">
          <div class="mr-4 back-btn mb-2 mb-sm-0 ">
            <%= render 'layouts/partials/shared/redirect_back', path: organisation_administration_portal_index_path %>
          </div>
          <h2 class="font-500 font-sm-18 font-25 text-capitalize"><%= @supplier.name %></h2>
        </div>
        <div class="right d-flex mt-3 mt-lg-0">
          <div class="white-btn d-flex align-items-center pl-2 pr-2 mr-2 py-2">
            <span><i class='fa fa-save font-16 mr-2'></i></span>
            <span class="text-capitalize font-12 font-xxl-14">save view</span>
          </div>
          <div class="white-btn d-flex align-items-center pl-2 pr-2 mr-2 py-2">
            <span> <i class="fa fa-sliders"></i></span>
            <span class="text-capitalize font-12 font-xxl-14 ml-2">Customised View</span>
          </div>
          <div class="white-btn p-1 d-flex align-items-center">
            <i class="fa fa-ellipsis-v px-2"></i>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="secondary-sidebar d-none">
    <i class='fa fa-ellipsis-v fa-1x font-18'></i>
  </div>
  <div class="row mt-5 no-gutters">
    <div class="col-sm-3 col-xxl-2 px-1">
      <aside class="fixed-secondary-sidebar position-static" id='sticky-sidebar-demo'>
        <i class="fa fa-times-circle fa-1x d-none"></i>
        <ul class="p-0 list-unstyled inner-wrapper-sticky">
          <li class="links active-white mb-2">
            <a href="#section1" class=" text-decoration-none text-capitalize font-13 font-xxl-14 d-block py-2">overview</a>
          </li>
          <li class="links mb-2">
            <a href="#section2" class=" text-decoration-none text-capitalize font-13 font-xxl-14 d-block py-2">Contracted Agreements</a>
          </li>
          <li class="links mb-2">
            <a href="#section3" class=" text-decoration-none text-capitalize font-13 font-xxl-14 d-block py-2">Aggregated Risk Posture</a>
          </li>
          <li class="links mb-2">
            <a href="#section4" class=" text-decoration-none text-capitalize font-13 font-xxl-14 d-block py-2">Service Level Indicators</a>
          </li>
          <li class="links mb-2">
            <a href="#section5" class=" text-decoration-none text-capitalize font-13 font-xxl-14 d-block py-2">Cloud Provider</a>
          </li>
          <li class="links mb-2">
            <a href="#section6" class=" text-decoration-none text-capitalize font-13 font-xxl-14 d-block py-2">4th party suppliers</a>
          </li>
          <li class="links mb-2">
            <a href="#section7" class=" text-decoration-none text-capitalize font-13 font-xxl-14 d-block py-2">Provider Resilience Gaps</a>
          </li>
        </ul>
      </aside>
    </div>
    <div class="col-sm-9 col-xxl-10 px-1">
      <div class="content-area">
        <div id="section1">
          <div class="card border-0 shadow-sm rounded-50 mb-4">
            <div class="card-body px-0">
              <div class="box">
                <div class="d-flex justify-content-between align-items-center mb-4 px-4">
                  <h5 class="text-capitalize text-left">Overview</h5>
                  <div class="text-right">
                    <a href="#" class="text-black">
                      <i class="fa fa-ellipsis-v text-right" aria-hidden="true"></i>
                    </a>
                  </div>
                </div>
                <div class="info-text mb-4 px-4">
                  <h6 class="mb-3 text-capitalize"><%= @supplier.name %></h6>
                  <p class="font-13">
                    <%= @supplier.description %>
                  </p>
                </div>
                <hr>
                <div class="details mb-4 px-4">
                  <div class="intro-box mb-3 d-flex align-items-center justify-content-center">
                    <div class="w-25">
                      <span class="font-12">Supplier Relationship Owner</span>
                    </div>
                    <div class="w-75">
                      <strong class="font-12"><%= @supplier.relationship_owner&.name %></strong>
                    </div>
                  </div>
                  <div class="intro-box mb-3 d-flex align-items-center justify-content-center">
                    <div class="w-25">
                      <span class="font-12">Supplier Key Contact</span>
                    </div>
                    <div class="w-75">
                      <strong class="w-75 font-12"><%= @supplier.key_contacts_list %></strong>
                    </div>
                  </div>
                  <div class="intro-box mb-3 d-flex align-items-center justify-content-center">
                    <div class="w-25">
                      <span class="font-12">Supplier Key Contact Details</span>
                    </div>
                    <div class="w-75">
                      <strong class="font-12">
                        <%= @supplier.key_contacts.pluck(:email).join(', ') %>
                      </strong>
                    </div>
                  </div>
                  <div class="intro-box mb-3 d-flex align-items-center justify-content-center">
                    <div class="w-25">
                      <span class="font-12">Supplier Type:</span>
                    </div>
                    <div class="w-75">
                      <strong class="font-12"><%= @supplier.consumption_model&.upcase %></strong>
                    </div>
                  </div>
                  <div class="intro-box mb-3 d-flex align-items-center justify-content-center">
                    <div class="w-25">
                      <span class="font-12">Contact Type:</span>
                    </div>
                    <div class="w-75">
                      <strong class="font-12"><%= @supplier.contracting_terms&.capitalize %></strong>
                    </div>
                  </div>
                  <div class="intro-box mb-3 d-flex align-items-center justify-content-center">
                    <div class="w-25">
                      <span class="font-12">Start Date & End Date</span>
                    </div>
                    <div class="w-75">
                      <strong class="font-12">
                        <%= "#{@supplier.strf_attr('start_date')} - #{@supplier.strf_attr('end_date')}" %>
                      </strong>
                    </div>
                  </div>
                  <div class="intro-box mb-3 d-flex align-items-center justify-content-center">
                    <div class="w-25">
                      <span class="font-12">Annual Cost of Contract</span>
                    </div>
                    <div class="w-75">
                      <strong class="font-12"><%= @supplier.annual_cost_of_contract %></strong>
                    </div>
                  </div>
                </div>
                <div class="px-4">
                  <h5 class="text-capitalize d-block mb-4">Links</h5>
                  <div class="small-boxes row">
                    <% @supplier.social_account_recipients.each do |social_account_recipient| %>
                      <div class="col-md-3 mb-3">
                        <%= link_to social_account_recipient.link, target: '_blank', class: 'text-decoration-none text-black' do %>
                            <div class="card">
                              <div class="card-body p-2 d-block rounded">
                                <div class="color-tier">
                                  <div class="mr-1 d-inline-block w-25px text-center align-middle">
                                    <% social_account = social_account_recipient.social_account %>
                                    <%= image_tag social_account.logo, class: 'img-fluid w-100' %>
                                  </div>
                                  <div class="text d-inline-block align-middle">
                                    <span class="font-12 font-400 ml-2 d-block">
                                      <%= social_account.name %>
                                    </span>
                                  </div>
                                </div>
                              </div>
                            </div>
                        <% end %>
                      </div>
                    <% end %>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div id="section2">
