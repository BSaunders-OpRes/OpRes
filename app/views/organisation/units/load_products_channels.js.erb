<% if @products %>
  $("#<%= params[:append_products_to] %>").html("<%= j(render 'organisation/units/partials/check_box', param_name: params[:product_param_name], options: @products.map{ |product| [product.name, product.id] }, selected_values: {}) %>");
  $("#<%= params[:append_channels_to] %>").html("<%= j(render 'organisation/units/partials/check_box', param_name: params[:channel_param_name], options: @channels.map{ |channel| [channel.name, channel.id] }, selected_values: {}) %>");
<% else %>
  $("#<%= params[:append_products_to] %>").html("<%= j(render 'organisation/units/partials/check_box', param_name: params[:product_param_name], options: {}, val: {} ) %>");
  $("#<%= params[:append_channels_to] %>").html("<%= j(render 'organisation/units/partials/check_box', param_name: params[:channel_param_name], options: {}, val: {} ) %>");
<% end %>
