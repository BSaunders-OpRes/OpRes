<div class="modal fade add-contact-modal bsl-step-supplier-modal my-5 py-5" id="add-contact-modal" tabindex="-1" role="dialog" aria-labelledby="new-supplier-modal-1" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header border-0 p-4">
        <h5 class="modal-title text-capitalize font-16">
          Add Contact
        </h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>

      <div class="modal-body p-4">
        <%= render 'organisation/key_contacts/partials/form', key_contact: @key_contact %>
      </div>
    </div>
  </div>
</div>
